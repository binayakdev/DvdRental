@model IEnumerable<Semester2Coursework.Models.AlbumProducer>

@{
    ViewBag.Title = "DvdDetails";
}

<h2>DvdDetails</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Albums.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Producers.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Albums.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Producers.Name)
        </td>
    </tr>
}

</table>
